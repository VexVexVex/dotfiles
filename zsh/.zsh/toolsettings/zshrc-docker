docker_login() {
  if [ -z $AWS_PROFILE ]; then
    echo "Select AWS Profile first..."
    return
  fi

  if [ -z $1 ]; then
    REGION="us-east-1"
  else
    echo "Using region $1"
    REGION=$1
  fi

  PASSWORD=`aws ecr get-login-password --region $REGION`
  ACCOUNTID=`aws sts get-caller-identity --output=text | awk '{print $1}'`

  docker login -u AWS -p $PASSWORD https://$ACCOUNTID.dkr.ecr.$REGION.amazonaws.com
  eval $DOCKER_KEY
}